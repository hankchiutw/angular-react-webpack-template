{
  "name": "angular-webpack-demo",
  "version": "1.1.0",
  "description": "Simple SPA using angular.js, webpack and react",
  "main": "src/public/js/main.js",
  "config": {},
  "scripts": {
    "start": "webpack-dev-server --progress --color --content-base docs/ --open --inline",
